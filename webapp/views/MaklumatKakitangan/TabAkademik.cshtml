                                        <label class="input">
                                            <input type='hidden' name='HR_MAKLUMAT_AKTIVITI[@item].HR_TARIKH_AKTIVITI' class='form-control HR_MAKLUMAT_AKTIVITI_HR_TARIKH_AKTIVITI' value='@string.Format("{0:dd/MM/yyyy}", Model.HR_MAKLUMAT_AKTIVITI.ElementAt(item).HR_TARIKH_AKTIVITI)' disabled="disabled" data-msg-required='Sila masukkan tarikh aktiviti' />
                                        </label>
                                    </td>
                                    <td>
                                        <div id="rowtxt">@Model.HR_MAKLUMAT_AKTIVITI.ElementAt(item).HR_NAMA_AKTIVITI</div>
                                        <label class="input">
                                            <input type='hidden' name='HR_MAKLUMAT_AKTIVITI[@item].HR_NAMA_AKTIVITI' class='form-control HR_MAKLUMAT_AKTIVITI_HR_NAMA_AKTIVITI' value='@Model.HR_MAKLUMAT_AKTIVITI.ElementAt(item).HR_NAMA_AKTIVITI' disabled="disabled" data-msg-required='Sila masukkan nama aktiviti' />
                                        </label>
                                    </td>
                                    <td>
                                        <div id="rowtxt">@Model.HR_MAKLUMAT_AKTIVITI.ElementAt(item).HR_ANJURAN</div>
                                        <label class="input">
                                            <input type='hidden' name='HR_MAKLUMAT_AKTIVITI[@item].HR_ANJURAN' class='form-control HR_MAKLUMAT_AKTIVITI_HR_ANJURAN' value='@Model.HR_MAKLUMAT_AKTIVITI.ElementAt(item).HR_ANJURAN' disabled="disabled" data-msg-required='Sila masukkan anjuran' />
                                        </label>
                                    </td>
                                    <td align="center">
                                        <button class="btn btn-danger btn-remove-AktivitiLuar" id="btn-remove-AktivitiLuar" name="remove-AktivitiLuar" type="button" disabled="disabled">
                                            <i class="fa fa-minus"></i>
                                        </button>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>

        </fieldset>
    </div>
    <br />
    <br />
